{
  "name": "project-homepage",
  "engines": {
    "npm": "⚠️️  【bun】 を使ってください",
    "pnpm": "⚠️️  【bun】 を使ってください",
    "yarn": "⚠️️  【bun】 を使ってください"
  },
  "scripts": {
    "check-code": "run-p -c lint spell-check",
    "commitlint": "commitlint --edit",
    "fix": "run-p fix:* -c",
    "fix:config": "prettier \"./config/**/*\" \"./*.{js,ts,mjs,csj}\" --write --log-level warn",
    "fix:pack": "fixpack",
    "lint": "run-p lint:* -c",
    "lint:config": "prettier \"./config/**/*\" \"./*.{js,ts,mjs,csj}\" --check --log-level warn",
    "lint:editorconfigChecker": "ec",
    "lint:pack": "fixpack --dryRun",
    "prepare": "husky",
    "spell-check": "cspell \"./config/**\" --no-progress"
  },
  "devDependencies": {
    "@commitlint/cli": "19.2.1",
    "@commitlint/config-conventional": "19.1.0",
    "@stylistic/eslint-plugin": "1.7.0",
    "@types/bun": "latest",
    "@typescript-eslint/eslint-plugin": "7.5.0",
    "cspell": "8.6.1",
    "editorconfig-checker": "5.1.5",
    "eslint": "9.0.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-next": "14.1.4",
    "eslint-config-prettier": "9.1.0",
    "eslint-config-strict-check": "0.0.24",
    "eslint-import-resolver-typescript-bun": "0.0.83",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-jest": "27.9.0",
    "eslint-plugin-jsx-a11y": "6.8.0",
    "eslint-plugin-react": "7.34.1",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-sonarjs": "0.25.1",
    "eslint-plugin-storybook": "0.8.0",
    "eslint-plugin-strict-check": "0.0.20",
    "eslint-plugin-strict-dependencies": "1.3.4",
    "eslint-plugin-unicorn": "52.0.0",
    "fixpack": "4.0.0",
    "husky": "9.0.11",
    "npm-run-all2": "6.1.2",
    "prettier": "3.2.5",
    "typescript": "5.4.4"
  },
  "peerDependencies": {
    "typescript": "5.4.4"
  },
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "type": "module"
}
