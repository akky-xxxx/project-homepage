{
  "name": "service-main",
  "version": "0.0.0",
  "module": "index.ts",
  "type": "module",
  "devDependencies": {
    "@pandacss/dev": "0.37.2",
    "@storybook/react": "8.0.6",
    "@testing-library/react": "14.2.2",
    "@types/bun": "latest",
    "@types/js-cookie": "3.0.6",
    "@types/react": "18.2.74",
    "@types/react-dom": "18.2.24",
    "storybook": "8.0.6"
  },
  "scripts": {
    "build": "next build",
    "build-storybook": "storybook build",
    "capture": "run-s capture:run-storybook capture:run-capture",
    "capture:run-capture": "storycap http://localhost:3015 -C \"puppeteer\" --disableCssAnimation --serverTimeout 120000 --captureTimeout 30000",
    "capture:run-storybook": "storybook dev --no-open -p 3015 -s ../public --quiet --no-version-updates",
    "check-code": "npm-run-all -p lint spell-check type-check -c",
    "dev": "next dev -p 3000",
    "fix": "run-p \"fix:*\" -c",
    "fix-ts-by-eslint": "eslint \"./{src,mock}/**/*.{ts,tsx}\" --fix --max-warnings 0 --cache",
    "fix-ts-by-prettier": "prettier \"./{src,mock}/**/*.{ts,tsx}\" --write --log-level warn",
    "fix:config": "prettier \"./.storybook/**/*\" \"./*.{js,mjs,cjs}\" --write --log-level warn",
    "fix:pack": "fixpack",
    "fix:ts": "run-s fix-ts-by-prettier fix-ts-by-eslint",
    "lint": "run-p \"lint:*\" -c",
    "lint-ts-by-eslint": "eslint \"./{src,mock}/**/*.{ts,tsx}\" --max-warnings 0 --cache",
    "lint-ts-by-prettier": "prettier \"./{src,mock}/**/*.{ts,tsx}\" --check --log-level warn",
    "lint:config": "prettier \"./.storybook/**/*\" \"./*.{js,mjs,cjs}\" --check --log-level warn",
    "lint:editorconfig": "editorconfig-checker",
    "lint:markup": "markuplint \"./src/**/*.{ts,tsx}\" -p",
    "lint:pack": "fixpack --dryRun",
    "lint:ts": "run-s lint-ts-by-prettier lint-ts-by-eslint",
    "spell-check": "cspell \"./{.storybook,config,src}/**\" --no-progress",
    "start": "next start -p 3000",
    "storybook": "storybook dev -p 3010",
    "type-check": "tsc"
  },
  "peerDependencies": {
    "typescript": "5.4.4"
  },
  "dependencies": {
    "jotai": "2.7.2",
    "js-cookie": "3.0.5",
    "next": "14.1.4",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "zod": "3.22.4"
  }
}
